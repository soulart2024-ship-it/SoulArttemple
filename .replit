entrypoint = "index.html"
modules = ["web", "python-3.11"]

[nix]
channel = "stable-24_11"
packages = ["static-web-server"]

[deployment]
publicDir = "/"
deploymentTarget = "static"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "SoulArt Web Server"

[[workflows.workflow]]
name = "SoulArt Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m http.server 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 80
externalPort = 80

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34021
externalPort = 3001

[[ports]]
localPort = 44003
externalPort = 3000

[agent]
expertMode = true
